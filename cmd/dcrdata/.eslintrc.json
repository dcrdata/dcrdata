{
    "extends": "standard",
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-trailing-spaces": "error",
        "no-console": ["off"],
        "no-alert": "error",
        "no-eval": "error",
        "no-implied-eval": "error",
        "no-var": "warn", // TODO: turn to error (default)
        "array-callback-return": "off"
    }
}